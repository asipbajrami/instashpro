# InstashPro - Development Environment (Docker)
# Usage: docker compose -f docker-compose.dev.yml --env-file .env.dev up
# Copy this file to .env.dev and fill in your API keys

# App Configuration
APP_NAME=InstashPro
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost:8000

# Logging
LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Database (Docker MySQL container)
DB_CONNECTION=mysql
DB_HOST=mysql
DB_PORT=3306
DB_DATABASE=instashpro
DB_USERNAME=instashpro
DB_PASSWORD=instashpro_password

# Set to true on first run to seed database, then set to false
SEED_DATABASE=false

# Cache & Queue (using file/database, no Redis)
BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database
SESSION_DRIVER=file
SESSION_LIFETIME=120

# Typesense
TYPESENSE_API_KEY=xyz

# Frontend URLs (for CORS)
FRONTEND_URL=http://localhost:3000
ADMIN_URL=http://localhost:5173

# Instagram Scraper API (RapidAPI)
INSTAGRAM_SCRAPER_API_KEY=
INSTAGRAM_SCRAPER_API_HOST=instagram-scraper-api2.p.rapidapi.com
INSTAGRAM_SCRAPER_BASE_URL=https://instagram-scraper-api2.p.rapidapi.com/v1/

# OpenRouter API (LLM for product extraction)
OPENROUTER_API_KEY=
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1/chat/completions
OPENROUTER_DEFAULT_MODEL=google/gemini-2.5-flash-preview-09-2025

# OpenAI
OPENAI_API_KEY=

# Anthropic
ANTHROPIC_API_KEY=

# Google
GOOGLE_API_KEY=

# Embedding Service (LiteLLM)
EMBEDDING_ENABLED=false
EMBEDDING_URL=
EMBEDDING_API_KEY=
EMBEDDING_TEXT_MODEL=qwen3-embedding
EMBEDDING_IMAGE_MODEL=siglip2-embedding
LITELLM_EMBEDDING_URL=
LITELLM_API_KEY=
