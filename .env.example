APP_NAME=InstashPro
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost:8000

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=instashpro
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database

# ===========================================
# Instagram Scraper API (RapidAPI)
# ===========================================
INSTAGRAM_SCRAPER_API_KEY=your_rapidapi_key_here
INSTAGRAM_SCRAPER_API_HOST=instagram-scraper-api2.p.rapidapi.com
INSTAGRAM_SCRAPER_BASE_URL=https://instagram-scraper-api2.p.rapidapi.com/v1.2

# ===========================================
# OpenRouter API (LLM for product extraction)
# ===========================================
OPENROUTER_API_KEY=your_openrouter_key_here
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1/chat/completions
OPENROUTER_DEFAULT_MODEL=google/gemini-2.5-flash-preview-09-2025

# ===========================================
# Typesense (Vector Search)
# ===========================================
TYPESENSE_API_KEY=xyz
TYPESENSE_HOST=localhost
TYPESENSE_PORT=8108
TYPESENSE_PROTOCOL=http

# ===========================================
# Embedding Service (LiteLLM)
# ===========================================
EMBEDDING_ENABLED=false
EMBEDDING_URL=http://localhost:4000
EMBEDDING_API_KEY=your_litellm_key_here
EMBEDDING_TEXT_MODEL=qwen3-embedding
EMBEDDING_IMAGE_MODEL=siglip2-embedding

# ===========================================
# Laravel Scout
# ===========================================
SCOUT_DRIVER=typesense

VITE_APP_NAME="${APP_NAME}"
